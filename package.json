    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "storybook": "^9.1.5",
    "typescript": "~5.8.0",
    "vite": "^7.0.6",
    "vite-plugin-dts": "^4.5.4",
