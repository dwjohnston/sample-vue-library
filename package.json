{
  "name": "sample-vue-library",
  "version": "0.0.3",
  "private": false,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "files": ["dist"],
  "module": "./dist/main.es.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/main.es.d.ts",
        "default": "./dist/main.es.js"
      }
